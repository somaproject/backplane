set -tmpdir build
set -xsthdpdir ./xst
run
-ifn boot.prj
-ifmt mixed
-ofn boot
-ofmt NGC
-p XC2C64A-7-VQ44
-top boot
-opt_mode Area
-opt_level 2
-iuc NO
-keep_hierarchy Yes
-rtlview Yes
-read_cores YES
-hierarchy_separator /
-bus_delimiter <>
-case maintain
-verilog2001 YES
-fsm_extract YES -fsm_encoding Auto
-safe_implementation YES
-mux_extract YES
-resource_sharing YES
-iobuf YES
-equivalent_register_removal YES
-pld_mp YES
-pld_xp YES
